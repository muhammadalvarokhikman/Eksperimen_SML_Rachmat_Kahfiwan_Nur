name: gizi-classification

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 3}
      learning_rate: {type: float, default: 0.1}
    command: "python modelling.py --n_estimators {n_estimators} --max_depth {max_depth} --learning_rate {learning_rate}"

python_env: 
  requirements: requirements.txt
